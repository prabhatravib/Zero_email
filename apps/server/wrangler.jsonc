{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pitext-mail",
  "compatibility_date": "2025-05-01",
  "compatibility_flags": ["nodejs_compat"],
  "main": "src/main.ts",
  "ai": {
    "binding": "AI",
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "ZERO_AGENT",
        "class_name": "ZeroAgent",
      },
      {
        "name": "ZERO_MCP",
        "class_name": "ZeroMCP",
      },
      {
        "name": "ZERO_DB",
        "class_name": "ZeroDB",
      },
      {
        "name": "ZERO_DRIVER",
        "class_name": "ZeroDriver",
      },
    ],
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "ZeroAgent",
        "ZeroMCP",
        "ZeroDB",
        "ZeroDriver"
      ]
    }
  ],
  "vars": {
    "NODE_ENV": "production",
    "COOKIE_DOMAIN": "pitext-email.onrender.com",
    "VITE_PUBLIC_BACKEND_URL": "https://pitext-mail.prabhatravib.workers.dev",
    "VITE_PUBLIC_APP_URL": "https://pitext-email.onrender.com",
    "DISABLE_CALLS": "true",
    "ELEVENLABS_API_KEY": "1234567890",
    "VOICE_SECRET": "1234567890",
    "DROP_AGENT_TABLES": "false",
    "THREAD_SYNC_MAX_COUNT": "20",
    "THREAD_SYNC_LOOP": "true",
    "DISABLE_WORKFLOWS": "true",
    "BETTER_AUTH_SECRET": "412222f1901b6a88402bf8eb1f148faa",
    "BETTER_AUTH_URL": "https://pitext-mail.prabhatravib.workers.dev",
    "GOOGLE_REDIRECT_URI": "https://pitext-mail.prabhatravib.workers.dev/auth/callback/google",
    "AUTUMN_SECRET_KEY": "",
    "FLASK_SECRET_KEY": "your-flask-secret-key-here",
    "LOG_LEVEL": "debug",
    "OPENAI_API_KEY": "your-openai-api-key-here",
  },
}
